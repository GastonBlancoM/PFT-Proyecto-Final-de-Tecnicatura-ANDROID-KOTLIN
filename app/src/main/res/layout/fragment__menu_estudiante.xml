<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/menuEstudiante"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.login.profiles.Fragment_MenuEstudiante"
    android:background="#202732">


    <Button
        android:id="@+id/buttonListaReclamo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="340dp"
        android:text="Lista de Reclamos"
        app:layout_constraintBottom_toBottomOf="parent"
        tools:layout_editor_absoluteX="123dp" />

    <Button
        android:id="@+id/buttonAltaReclamo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Alta de Reclamos"
        app:layout_constraintBottom_toTopOf="@+id/buttonListaReclamo"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.738"
        tools:layout_editor_absoluteX="128dp" />
</androidx.constraintlayout.widget.ConstraintLayout>